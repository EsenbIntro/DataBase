You are now connected to database "basic_operations" as user "postgres".
basic_operations=#

basic_operations=# CREATE TABLE Students (
basic_operations(#      student_id SERIAL PRIMARY KEY,
basic_operations(#      first_name VARCHAR(50),
basic_operations(#      last_name VARCHAR(50),
basic_operations(#      birth_date DATE
basic_operations(# );
CREATE TABLE

