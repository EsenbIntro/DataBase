library=# CREATE TABLE loans (
library(#       loan_id SERIAL PRIMARY KEY,
library(#       member_id INTEGER NOT NULL,
library(#       book_id INTEGER NOT NULL,
library(#       loan_date DATE DEFAULT CURRENT_DATE,
library(#       due_date DATE NOT NULL,
library(#       return_date DATE,
library(#       late_fee DECIMAL(10,2) DEFAULT 0.00,
library(#       FOREIGN KEY (member_id) REFERENCES members(member_id),
library(#       FOREIGN KEY (book_id) REFERENCES books(book_id)
library(# );
