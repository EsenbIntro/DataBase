university_design=# CREATE TABLE courses (
university_design(#     course_id SERIAL PRIMARY KEY,
university_design(#     course_name VARCHAR(100) UNIQUE NOT NULL,
university_design(#     credits INT NOT NULL,
university_design(#     description TEXT,
university_design(#     professor_id INTEGER NOT NULL,
university_design(#     FOREIGN KEY (professor_id) REFERENCES professors(professor_id) ON DELETE CASCADE
university_design(# );
CREATE TABLE

university_design=# CREATE TABLE student_enrollments (
university_design(#     enrollment_id SERIAL PRIMARY KEY,
university_design(#     student_id INTEGER NOT NULL,
university_design(#     course_id INTEGER NOT NULL,
university_design(#     FOREIGN KEY (student_id) REFERENCES students(student_id),
university_design(#     FOREIGN KEY (course_id) REFERENCES courses(course_id),
university_design(#     UNIQUE(student_id, course_id)
university_design(# );
CREATE TABLE

